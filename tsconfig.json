{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noImplicitAny": false,
    },
    "moduleResolution": "node",
    "include": ["src/**/*", "api/**/*", "types.d.ts"],
    "exclude": ["node_modules"]
  }
  